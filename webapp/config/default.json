        "video": {
            "collections": [],
            "extensions": [".mp4", ".ogv", ".wbem"],
            "schemes": {
                "cdn": "https://cdn.kidoju.com/"
            }
        }
    },
    "awss3": {
        "region": "us-east-1",
        "config": {
            "bucket": "kidoju.config",
            "key": "kidoju.webapp/production.json"
        }
    },
    "head" : "<meta name=\"p:domain_verify\" content=\"00a8ab216c21a6cd727d5d50881ceb1e\"/>",
    "images": ["https://cdn.kidoju.com/kidoju/home1.jpg", "https://cdn.kidoju.com/kidoju/home2.jpg", "https://cdn.kidoju.com/kidoju/home3.jpg"],
    "locales" : [ "en", "fr" ],
    "uris" : {
        "rapi": {
            "v1": {
                "activities": "/api/v1/%s/summaries/%s/activities",
                "files": "/api/v1/%s/summaries/%s/files",
                "summary": "/api/v1/%s/summaries/%s",
                "summaries": "/api/v1/%s/summaries",
                "user": "/api/v1/users/%s"
            }
        },
        "cdn": {
            "root":  "https://cdn.kidoju.com",
            "icons": "/images/o_collection/svg/office/%s.svg",
            "images": "/kidoju/%s"
        },
        "webapp": {
            "editor": "/%s/e/%s/%s",
            "error": "/%s/error",
            "finder": "/%s",
            "home": "/",
            "locale": "/%s",
            "ping": "/ping",
            "player": "/%s/x/%s/%s/%s",
            "public": "/%s",
            "rss": "/%s/rss/%s",
            "sitemap": "/%s/sitemap/%s",
            "summary": "/%s/s/%s",
            "support": "/support/%s",
            "user": "/%s/u/%s",
            "workerlib": "workerlib.bundle.js"
        }
    }
